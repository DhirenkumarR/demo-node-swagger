        "/api/v1/auth/login": {
            "post": {
                "summary": "User Login",
                "tags": [
                    "Auth"
                ],
                "requestBody": {
                    "description": "Login crendentials",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "example": "User@datebookai.com"
                                    },
                                    "mobile_number": {
                                        "type": "string",
                                        "example": "9876543210"
                                    },
                                    "country_code": {
                                        "type": "string",
                                        "example": "+91"
                                    },
                                    "firebase_token": {
                                        "type": "string",
                                        "example": "abcd.....ac"
                                    },
                                    "device_type": {
                                        "type": "string",
                                        "example": "Android/iOS"
                                    },
                                    "device_os": {
                                        "type": "string",
                                        "example": "zxy..."
                                    },
                                    "device_name": {
                                        "type": "string",
                                        "example": "acd..."
                                    },
                                    "login_type": {
                                        "type": "string",
                                        "enum": [
                                            "google",
                                            "apple",
                                            "instagram",
                                            "facebook",
                                            "tiktok",
                                            "normal"
                                        ],
                                        "example": "facebook"
                                    },
                                    "device_id": {
                                        "type": "string",
                                        "example": "ABDSJBSAJB"
                                    }
                                },
                                "required": [
                                    "email",
                                    "mobile_number"
                                ]
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Login successfully."
                    },
                    "400": {
                        "description": "Email and mobile_number are invalid."
                    },
                    "404": {
                        "description": "The user account is deactivated. Please contact support for further assistance."
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        },
