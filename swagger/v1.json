                                        "type": "string",
                                        "example": "No"
                                    },
                                    "distance_range": {
                                        "type": "string",
                                        "example": "0-50"
                                    },
                                    "units": {
                                        "type": "string",
                                        "example": "km/miles"
                                    },
                                    "search_address": {
                                        "type": "string",
                                        "example": "Dubai"
                                    },
                                    "city": {
                                        "type": "string",
                                        "example": "Dubai"
                                    },
                                    "state": {
                                        "type": "string",
                                        "example": "UAE"
                                    },
                                    "country": {
                                        "type": "string",
                                        "example": "UAE"
                                    },
                                    "zipcode": {
                                        "type": "string",
                                        "example": "12345"
                                    },
                                    "lat": {
                                        "type": "string",
                                        "example": "25.2048"
                                    },
                                    "long": {
                                        "type": "string",
                                        "example": "55.2708"
                                    },
                                    "relationship_id": {
                                        "type": "string",
                                        "example": "6736df52aeee7c16e0018d2b"
                                    },
                                    "hobbies_id": {
                                        "type": "string",
                                        "example": "6736df52aeee7c16e0018d0f"
                                    },
                                    "bio_description": {
                                        "type": "string",
                                        "example": "Good"
                                    },
                                    "user_image": {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary",
                                            "description": "Multiple image files"
                                        },
                                        "description": "Array of image files",
                                        "nullable": true
                                    },
                                    "user_video": {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary",
                                            "description": "Multiple video files"
                                        },
                                        "description": "Array of video files",
                                        "nullable": true
                                    },
                                    "instagram_connected": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "instagram_token": {
                                        "type": "string",
                                        "example": "token..."
                                    },
                                    "facebook_connected": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "facebook_token": {
                                        "type": "string",
                                        "example": "token..."
                                    },
                                    "tiktok_connected": {
                                        "type": "boolean",
                                        "example": true
                                    },
                                    "tiktok_token": {
                                        "type": "string",
                                        "example": "token..."
                                    }
                                }
                            }
                        }
                    }
